# Listen on port
APP_PORT="3000"

# CORS allowed origin  ( e.g. http://example.com,http://another.com, * = allow all)
# อนุญาตให้ตั้งค่า origin ได้หลายตัวคั่นด้วยคอมมา หรือใช้ "*" เพื่อทั้งหมด
CORS_ORIGIN="*"

# Environment (development, production, test)
NODE_ENV="development"

# Log level ("fatal", "error", "warn", "info", "debug", "trace", "silent")
LOG_LEVEL="debug"

# Verbose log for test (1 = on, 0 = off)
TEST_VERBOSE_LOG="0"

# Force exit after timeout (ms)
FORCE_EXIT_MS="10000"